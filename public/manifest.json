{
  "name": "Convenient Homepage",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Manage your notes and links directly on your browser homepage",
  "icons": {
    "192": "logo192.png"
  },
  "permissions": ["bookmarks", "identity", "alarms", "storage"],
  "host_permissions": ["https://*/*"],
  "background": {
    "service_worker": "static/js/background.js"
  },
  "action": {
    "default_icon": "logo192.png",
    "default_popup": "popup.html"
  },
  "options_page": "index.html",
  "oauth2": {
    "client_id": "",
    "scopes": [""]
  },
  "key": ""
}
