{
  "name": "Convenient Homepage",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Manage your notes and links directly on your browser homepage",
  "icons": {
    "192": "logo192.png"
  },
  "permissions": ["bookmarks", "https://*/*"],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "logo192.png",
    "default_popup": "popup.html"
  },
  "options_page": "index.html"
}
